<resources>
    <string id="AppName">NightscoutWidget</string>
    <string id="SiteName">Site Name</string>
    <string id="AppVersionTitle">Application Version</string>
 
<bitmap id="LauncherIcon" filename="images/nightscout.png" />
    
    <menu id="MainMenu">
  <menu-item id="item_2">Graphs</menu-item>
  <menu-item id="item_1">Settings</menu-item>
</menu>

    <menu id="SettingsMenu">
  <menu-item id="item_1">Site Name</menu-item>
  <menu-item id="item_2">Host Domain</menu-item>
  <menu-item id="item_3">Units</menu-item>
  <menu-item id="item_4">Pick a site</menu-item>
</menu>
  
    <menu id="HostMenu">
  <menu-item id="item_1">azurewebsites.net</menu-item>
  <menu-item id="item_2">herokuapp.com</menu-item>
</menu>

    <menu id="UnitsMenu">
  <menu-item id="item_1">mmol/L</menu-item>
  <menu-item id="item_2">mg/dL</menu-item>
</menu>

    <menu id="SiteSelectMenu">
  <menu-item id="item_1">Dragon</menu-item>
  <menu-item id="item_2">Snake</menu-item>
  <menu-item id="item_3">Horse</menu-item>
</menu>

 
<properties>
    <property id="appVersion" type="string">0.0.8</property>
    <property id="sitename" type="string">phimbycgm</property>
</properties>


<settings>

    <setting propertyKey="@Properties.appVersion" title="@Strings.AppVersionTitle">
        <settingConfig type="alphaNumeric" readonly="true" />
    </setting>

    <setting propertyKey="@Properties.sitename" title="@Strings.SiteName">
        <settingConfig type="alphaNumeric" required="true" />
    </setting>


</settings>

<!--
